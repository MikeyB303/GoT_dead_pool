<h1>Characters!!!</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
    </tr>
  </thead>

  <tbody>
    <% @characters.each do |character| %>
    <tr>
      <td><%= character.name %></td>
      <td><%= character.character_status.status_label %></td>
    </tr>
    <% end %>
  </tbody>
</table>